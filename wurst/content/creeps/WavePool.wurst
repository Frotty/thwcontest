package WavePool
import LinkedList
import Creep

class SpawnContainer
	int creepId
	int amount
	int time
	
	construct(int id, int am, int tm)
		creepId = id
		amount = am
		time = tm


public class WavePool
	LinkedList<SpawnContainer> spawns	

	function addSpawn(int creepId, int amount, int time)
		spawns.add(new SpawnContainer(creepId, amount, time))