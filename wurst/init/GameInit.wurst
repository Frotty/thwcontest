package GameInit
import MageHero
import ClosureTimers

function initResources(boolean compileTime)
	init_MageHero(compileTime)	
	
	
init
	doAfter(0.01, () -> begin
		initResources(false)
		createUnit(Player(0), 'hfoo', vec2(0,0), angle(0))
	end)	
	
@compiletime function generateObjects()
	initResources(true)